<?xml version="1.0"?>

<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <carriers>
            <gls>
                <active>0</active>
                <client_id/>
                <api_mode>sandbox</api_mode>
                <api_username/>
                <api_password backend_model="Magento\Config\Model\Config\Backend\Encrypted"/>
                <api_country>HR</api_country>
                <debug>0</debug>
                <title>GLS</title>
                <max_package_weight>40</max_package_weight>
                <sort_order/>
                <allowed_methods>standard,psd</allowed_methods>
                <standard_method_name>Delivery to Address</standard_method_name>
                <standard_method_price>0</standard_method_price>
                <psd_sallowspecific>0</psd_sallowspecific>
                <psd_specificcountry/>
                <psd_method_name>Delivery to Parcel Location</psd_method_name>
                <psd_method_price>0</psd_method_price>
                <psd_sallowspecific>0</psd_sallowspecific>
                <psd_specificcountry/>
                <client_reference>Order #{increment_id}</client_reference>
                <printer_type>A4_2x2</printer_type>
                <print_position>1</print_position>
                <guaranteed_24h>0</guaranteed_24h>
                <express_delivery/>
                <cs1>0</cs1>
                <fds>0</fds>
                <fss>0</fss>
                <sm1>0</sm1>
                <sm1_text/>
                <sm2>0</sm2>
                <aos>0</aos>
<!--                <ins>0</ins>-->
                <supported_countries>CZ,HR,HU,RO,SI,SK</supported_countries>
                <model>GLSCroatia\Shipping\Model\Carrier</model>
            </gls>
        </carriers>
    </default>
</config>
